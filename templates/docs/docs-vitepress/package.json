{
  "name": "docs-vitepress",
  "version": "0.0.0",
  "description": "A project created by docs-vitepress",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run build:esbuild",
    "dev": "npm run dev:esbuild",
    "dev:esbuild": "NODE_ENV=local npm run clean && npx npm-run-all --parallel build:esbuild:watch",
    "dev:rollup": "NODE_ENV=local npm run clean && npx npm-run-all --parallel build:rollup:watch",
    "dev:tsc": "NODE_ENV=local npm run clean &&  npm run build:tsc:watch -- --onSuccess \"npm start\"",
    "start": "node dist/main.js",
    "start:watch1": "npx nodemon dist/main.js",
    "start:watch": "npx nodemon --ext js --exec \"node dist/main.js\"",
    "start:tsc": "npx npm-run-all --parallel build:tsc start",
    "build:esbuild": "npm run clean && node esbuild.build.mjs",
    "build:esbuild:watch": "npm run clean && node esbuild.dev.mjs",
    "build:rollup": "npm run clean && rollup -c",
    "build:rollup:watch": "npm run clean && rollup -c -w",
    "build:tsc": "npm run clean && npx tsc -p tsconfig.build.json",
    "build:tsc:watch": "npm run clean && npx tsc-watch -p tsconfig.app.json",
    "lint-staged": "npx lint-staged",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run vitest:run",
    "test:e2e": "npm run vitest:e2e",
    "vitest": "vitest --passWithNoTests",
    "vitest:run": "vitest --passWithNoTests --run",
    "vitest:ui": "vitest --passWithNoTests --ui",
    "vitest:e2e": "vitest --config vitest.config.e2e.mts --passWithNoTests",
    "vitest:e2e:run": "vitest --config vitest.config.e2e.mts --passWithNoTests --run",
    "vitest:e2e:ui": "vitest --config vitest.config.e2e.mts --passWithNoTests --ui",
    "jest": "jest --passWithNoTests",
    "jest:watch": "jest --watch",
    "jest:web": "jest && npx http-server ./.test/jest",
    "jest:cov": "jest --verbose --coverage",
    "jest:cov:web": "npm run jest:cov && npx http-server ./.test/jest/coverage/lcov-report",
    "jest:e2e": "jest --config jest.config.e2e.ts --passWithNoTests",
    "jest:e2e:watch": "jest --config jest.config.e2e.ts --watch",
    "jest:e2e:web": "npm run jest:e2e && npx http-server ./.test/jest-e2e",
    "jest:e2e:cov": "jest --config jest.config.e2e.ts --verbose --coverage",
    "prepare": "npx husky",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "_moduleAliases": {
    "@": "./dist"
  },
  "lint-staged": {
    "*.ts": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "Roy Chuang",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.24.0 <11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/dotenv-flow": "^3.3.3",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "inquirer": "9",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.0.1",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.36.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "dotenv-flow": "^4.1.0",
    "module-alias": "^2.2.3"
  }
}